         VALUE "LegalCopyright",   "(C) 2004-2007 innotek GmbH\0"
         VALUE "OriginalFilename", "VBoxService.exe\0"
         VALUE "ProductName",      "VirtualBox Additions Service\0"
         VALUE "ProductVersion",   VBOX_VERSION_MAJOR "." VBOX_VERSION_MINOR "." VBOX_VERSION_BUILD "\0"
      END
   END
   BLOCK "VarFileInfo"
   BEGIN
      VALUE "Translation", 0x409, 1200
   END
END

IDI_VIRTUALBOX ICON DISCARDABLE "vbox.ico"

