         VALUE "CompanyName",      "innotek GmbH"
         VALUE "FileDescription",  "VirtualBox Additions Service\0"
         VALUE "FileVersion",      VBOX_VERSION_MAJOR "." VBOX_VERSION_MINOR "." VBOX_VERSION_BUILD "\0"
