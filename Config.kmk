ifeq ($(KBUILD_TARGET),freebsd)
 VBOX_WITH_WEBSERVICES=
 VBOX_WITH_INSTALLER=
 VBOX_WITH_SECURELABEL=
 VBOX_WITH_VRDP_AUTHMOD=
 VBOX_WITH_VRDP_RDESKTOP=
 VBOX_WITH_TESTSUITE=
 VBOX_WITH_USB=
 VBOX_WITH_EHCI=
