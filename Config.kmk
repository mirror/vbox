TEMPLATE_VBOXMAINEXE_CFLAGS.x86          = -m32
TEMPLATE_VBOXMAINEXE_CFLAGS.amd64        = -m64
TEMPLATE_VBOXMAINEXE_CFLAGS.kprofile     = $(TEMPLATE_VBOXMAINEXE_CXXFLAGS.kprofile)
