   endif
  endif
  ifndef TEMPLATE_VBOXMAINCOMP_LDFLAGS
   TEMPLATE_VBOXMAINCOMP_LDFLAGS   = -fPIC $(TEMPLATE_VBOXMAINEXE_LDFLAGS)
