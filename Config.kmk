ifeq ($(KBUILD_TARGET),l4)
 VBOX_WITH_VBOXDRV=
 VBOX_WITH_ADDITION_DRIVERS=
 VBOX_WITH_VRDP=
 VBOX_WITH_HEADLESS=
 VBOX_WITH_VBOXSDL=
 VBOX_WITH_QTGUI=
 VBOX_WITH_MAIN=
 VBOX_WITH_DOCS=
 VBOX_WITH_ISCSI=
 VBOX_WITH_INIP=
 VBOX_WITH_INTERNAL_NETWORKING=
 VBOX_WITH_PDM_ASYNC_COMPLETION=
 VBOX_WITH_KCHMVIEWER=
 VBOX_WITH_HARDENING=
endif

ifeq ($(KBUILD_TARGET),os2)
 VBOX_WITH_WEBSERVICES=
 VBOX_WITH_INSTALLER=
 VBOX_WITH_SECURELABEL=
 VBOX_WITH_VRDP_AUTHMOD=
 VBOX_WITH_VRDP_RDESKTOP=
 VBOX_WITH_TESTSUITE=
 VBOX_WITH_QTGUI=
 VBOX_WITH_USB=
 VBOX_WITH_EHCI=
